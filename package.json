{
  "engines": {
    "node": "10.15.3"
  },
  "name": "announceit-rest-api",
  "version": "1.0.0",
  "description": "REST API for announceIT UI",
  "main": "src/app.js",
  "scripts": {
    "test": "mocha ./src/app.spec.js --require @babel/register --exit",
    "start:watch": "nodemon --exec babel-node src/app.js",
    "start": "babel-node src/app.js",
    "precover": "nyc --reporter=html --reporter=text npm run test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcover": "npm run precover && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karamuka/announceit-rest-api.git"
  },
  "keywords": [
    "REST",
    "API",
    "AnnounceIT"
  ],
  "author": "karamuka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karamuka/announceit-rest-api/issues"
  },
  "homepage": "https://github.com/karamuka/announceit-rest-api#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.0.0",
    "pg": "^7.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "istanbul": "^0.4.5",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0"
  }
}